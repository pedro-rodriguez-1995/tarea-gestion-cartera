<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.sinensia.controllers.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MainappController</servlet-name>
  	<display-name>MainappController</display-name>
  	<description></description>
  	<servlet-class>com.sinensia.controllers.MainappController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UsuarioFormController</servlet-name>
  	<display-name>UsuarioFormController</display-name>
  	<description></description>
  	<servlet-class>com.sinensia.controllers.UsuarioFormController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertarMovController</servlet-name>
  	<display-name>InsertarMovController</display-name>
  	<description></description>
  	<servlet-class>com.sinensia.controllers.InsertarMovController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DetalleController</servlet-name>
  	<display-name>DetalleController</display-name>
  	<description></description>
  	<servlet-class>com.sinensia.controllers.DetalleController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutController</servlet-name>
  	<display-name>LogoutController</display-name>
  	<description></description>
  	<servlet-class>com.sinensia.controllers.LogoutController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MainappController</servlet-name>
  	<url-pattern>/MainappController</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>UsuarioFormController</servlet-name>
  	<url-pattern>/UsuarioFormController</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>InsertarMovController</servlet-name>
   	<url-pattern>/InsertarMovController</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>DetalleController</servlet-name>
   	<url-pattern>/DetalleController</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>LogoutController</servlet-name>
   	<url-pattern>/LogoutController</url-pattern>
   </servlet-mapping>
  
  <welcome-file-list>
  	<welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
